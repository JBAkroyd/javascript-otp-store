{
  "name": "Javascript OTP Store",
  "description": "Scan a QR Code",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_title": "Click Me!",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}